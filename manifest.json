{
	"name": "Page printer",
	"description": "prod",
    "background": {
        "scripts": ["/js/background.js"]
		},
	"browser_action": {
		"default_title": "Page printer",
		"default_icon": "images/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"\u003Call_urls>"
			],
			"js": [
				"bootstrap/js/jquery.min.js",
				"bootstrap/js/bootstrap.min.js",
				"/js/content.js"
			],
			"css": [
				"css/index.css",
				"css/iframe.css"
			],
			"all_frames": true
		}
	],
	"permissions": [
		"tabs",
		"background",
		"storage",
		"<all_urls>",
		"webRequest",
		"webRequestBlocking",
		"contextMenus"
	],
	"web_accessible_resources": [
		"css/iframe.css",
		"css/index.css",
		"images/dustbin.png"
  ],
	"manifest_version": 2,
	"version": "1.0",
	"content_security_policy": "default-src 'self'; frame-src 'self'; script-src 'self' 'unsafe-eval'; object-src 'self'",
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Ctrl+Shift+F"
			}
		}
	}
}