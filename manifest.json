{
	"name": "Page printer",
	"description": "prod",
    "background": {
        "scripts": ["/js/background.js"]
		},
	"browser_action": {
		"default_title": "Page printer",
		"default_icon": "images/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"\u003Call_urls>"
			],
			"js": [
				"bootstrap/js/jquery.min.js",
				"bootstrap/js/bootstrap.min.js",
				"/js/content.js"
			],
			"css": [
				"js/css/content.css",
				"bootstrap/css/bootstrap.min.css",
				"bootstrap/css/bootstrap-theme.min.css"
			],
			"all_frames": true
		}
	],
	"permissions": [
        "activeTab",
		"tabs",
		"background",
		"storage",
		"<all_urls>",
		"webRequest",
		"webRequestBlocking"
	],
	"manifest_version": 2,
	"version": "1.0"
}
